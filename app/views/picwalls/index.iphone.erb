<% @picwalls_iphone.each do |picwall| %>
<%= link_to (image_tag picwall.postpic.url(:thumb), :class => 'picwall-postpic'), picwall.postpic.url %>
 <% picwall.comments.each do |comment| %>
 <%= link_to (image_tag comment.commentpic.url(:thumb), :class => 'picwall-commentpic'), comment.commentpic.url %>
 <% end %>
<% end %>
